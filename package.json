{
  "name": "mint.club-cli",
  "version": "1.0.4",
  "description": "CLI for Mint Club V2 bonding curve tokens",
  "main": "dist/index.js",
  "files": ["dist", "README.md"],
  "bin": {
    "mc": "./dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --format esm --target node --minify",
    "prepublishOnly": "bun run build",
    "start": "bun src/index.ts",
    "check": "tsc --noEmit"
  },
  "keywords": [
    "mint",
    "club",
    "bonding",
    "curve",
    "crypto",
    "defi"
  ],
  "author": "h1-hunt <h1@hunt.town>",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "viem": "^2.21.49"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
